{
  "name": "Global Image downloader",
  "version": "0.0.2",
  "description": "Download images from any web page easily",
  "manifest_version": 3,
  "action": {
    "default_popup": "app.html"
  },
  "background": {},
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["image-page.js"]
    }
  ],
  "host_permissions": [],
  "permissions": [],
  "optional_permissions": [],
  "icons": {
    "128": "assets/icons/logo_128.png",
    "256": "assets/icons/logo_256.png",
    "512": "assets/icons/logo_512.png"
  }
}
